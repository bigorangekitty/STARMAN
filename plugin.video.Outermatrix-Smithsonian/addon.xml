<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.Outermatrix-Smithsonian"
	name="Smithsonian Channel"
	version="1.0.1"
	provider-name="Outermatrix.net">
  <requires>
    <import addon="xbmc.python" version="2.0"/>
    <import addon="plugin.video.youtube" version="3.0.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>video</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">E! Entertainment</summary>
    <description lang="en">Smithsonian Channel brings you original documentaries and videos exploring history, air and space, science, nature, and culture.</description>
    <platform>all</platform>
  </extension>
</addon>
